{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "harvest": {
      "type": "string",
      "required": true
    },
    "uses": {
      "type": "string",
      "required": true
    },
    "refills": {
      "type": "string",
      "required": true
    }
  }
}
